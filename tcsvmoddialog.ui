<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="TCsvModDialog" parent="GtkDialog">
    <property name="title">Modify fields</property>
    <property name="default-width">1000</property>
    <property name="default-height">800</property>
    <child internal-child="content_area">
      <object class="GtkBox">
        <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
        <child>
          <object class="GtkBox">
            <child>
              <object class="GtkLabel">
                <property name="width-chars">10</property>
              </object>
            </child>
            <child>
              <object class="GtkButton">
                <property name="label">Append new field</property>
                <signal name="clicked" handler="mod_app_cb" swapped="false" object="TCsvModDialog"></signal>
              </object>
            </child>
            <child>
              <object class="GtkButton">
                <property name="label">Insert new field before</property>
                <signal name="clicked" handler="mod_ins_cb" swapped="false" object="TCsvModDialog"></signal>
              </object>
            </child>
            <child>
              <object class="GtkEntry">
                <property name="buffer">
                  <object class="GtkEntryBuffer" id="entry_buffer">
                    <property name="text">0</property>
                  </object>
                </property>
              </object>
            </child>
            <child>
              <object class="GtkButton">
                <property name="label">Clean</property>
                <signal name="clicked" handler="mod_clean_cb" swapped="false" object="TCsvModDialog"></signal>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="hexpand">TRUE</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkScrolledWindow">
            <property name="margin-start">10</property>
            <property name="margin-end">10</property>
            <property name="margin-bottom">10</property>
            <property name="hexpand">TRUE</property>
            <property name="vexpand">TRUE</property>
            <child>
              <object class="GtkColumnView" id="columnview">
                <property name="hexpand">TRUE</property>
                <property name="vexpand">TRUE</property>
                <property name="focusable">FALSE</property>
                <property name="model">
                  <object class="GtkNoSelection">
                    <property name="model">
                      <object class="GListStore" id="liststore">
                        <property name="item-type">TModify</property>
                      </object>
                    </property>
                  </object>
                </property>
                <child>
                  <object class="GtkColumnViewColumn">
                    <property name="title">Up</property>
                    <property name="factory">
                      <object class="GtkBuilderListItemFactory">
                        <property name="bytes"><![CDATA[
<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="GtkListItem">
    <property name="child">
      <object class="GtkButton">
        <property name="label">↑</property>
        <property name="focusable">FALSE</property>
        <signal name="clicked" handler="mod_up_cb" swapped="false" object="GtkListItem"></signal>
      </object>
    </property>
  </template>
</interface>
                        ]]></property>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkColumnViewColumn">
                    <property name="title">Down</property>
                    <property name="factory">
                      <object class="GtkBuilderListItemFactory">
                        <property name="bytes"><![CDATA[
<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="GtkListItem">
    <property name="child">
      <object class="GtkButton">
        <property name="label">↓</property>
        <property name="focusable">FALSE</property>
        <signal name="clicked" handler="mod_down_cb" swapped="false" object="GtkListItem"></signal>
      </object>
    </property>
  </template>
</interface>
                        ]]></property>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkColumnViewColumn">
                    <property name="title">Old Position</property>
                    <property name="factory">
                      <object class="GtkBuilderListItemFactory">
                        <property name="bytes"><![CDATA[
<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="GtkListItem">
    <property name="child">
      <object class="GtkLabel">
        <property name="focusable">FALSE</property>
        <binding name="label">
          <lookup name="old-position" type="TModify">
            <lookup name="item">GtkListItem</lookup>
          </lookup>
        </binding>
      </object>
    </property>
  </template>
</interface>
                        ]]></property>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkColumnViewColumn">
                    <property name="title">Old field</property>
                    <property name="factory">
                      <object class="GtkBuilderListItemFactory">
                        <property name="bytes"><![CDATA[
<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="GtkListItem">
    <property name="child">
      <object class="GtkLabel">
        <property name="focusable">FALSE</property>
        <binding name="label">
          <lookup name="old-string" type="TModify">
            <lookup name="item">GtkListItem</lookup>
          </lookup>
        </binding>
      </object>
    </property>
  </template>
</interface>
                        ]]></property>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkColumnViewColumn">
                    <property name="title">State</property>
                    <property name="factory">
                      <object class="GtkBuilderListItemFactory">
                        <property name="bytes"><![CDATA[
<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="GtkListItem">
    <property name="child">
      <object class="GtkLabel">
        <property name="focusable">FALSE</property>
        <binding name="label">
          <closure type="gchararray" function="get_state">
            <lookup name="old-string" type="TModify">
              <lookup name="item">GtkListItem</lookup>
            </lookup>
            <lookup name="new-string" type="TModify">
              <lookup name="item">GtkListItem</lookup>
            </lookup>
          </closure>
        </binding>
      </object>
    </property>
  </template>
</interface>
                        ]]></property>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkColumnViewColumn">
                    <property name="title">New Position</property>
                    <property name="factory">
                      <object class="GtkBuilderListItemFactory">
                        <property name="bytes"><![CDATA[
<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="GtkListItem">
    <property name="child">
      <object class="GtkLabel">
        <property name="focusable">FALSE</property>
        <binding name="label">
          <lookup name="position">GtkListItem</lookup>
        </binding>
      </object>
    </property>
  </template>
</interface>
                        ]]></property>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkColumnViewColumn" id="column4">
                    <property name="title">New field</property>
                    <property name="expand">TRUE</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child type="action">
      <object class="GtkButton" id="btn_cancel">
        <property name="label">Cancel</property>
      </object>
    </child>
    <child type="action">
      <object class="GtkButton" id="btn_accept">
        <property name="label">Save</property>
      </object>
    </child>
    <action-widgets>
      <action-widget response="cancel">btn_cancel</action-widget>
      <action-widget response="accept" default="true">btn_accept</action-widget>
    </action-widgets>
  </template>
 </interface>

